{
  "name": "proof-of-concept-v2",
  "description": "",
  "version": "1.0.0",
  "main": "server/index.js",
  "author": "Mike Willamson",
  "license": "MIT",
  "scripts": {
    "extract": "lingui extract",
    "compile": "lingui compile",
    "add-locale": "lingui add-locale",
    "dev": "npm run clean && cross-env NODE_ENV=development babel-watch server/index.js",
    "dbg": "node --inspect-brk node_modules/jest/bin/jest.js",
    "start": "npm run serve",
    "serve": "cross-env NODE_ENV=production node buildServer/index.js",
    "build": "npm run build:client && npm run build:server && npm run build:node",
    "build:client": "rimraf buildClient && cross-env NODE_ENV=production webpack --progress -p --config webpack/client.prod.js",
    "build:server": "rimraf buildServer && cross-env NODE_ENV=production webpack --progress -p --config webpack/server.prod.js",
    "build:node": "cross-env NODE_ENV=production babel server/index.js -o buildServer/index.js",
    "clean": "rimraf buildClient buildServer",
    "precommit": "lint-staged",
    "cm": "git-cz",
    "lint": "eslint src server webpack",
    "format": "prettier --single-quote --semi=false --write '{src,server,webpack}/**/*.js' && npm run lint",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "@cdssnc/gcui": "^0.0.30",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-link-http": "^1.5.4",
    "emotion": "^9.2.4",
    "emotion-server": "^9.2.4",
    "emotion-theming": "^9.2.4",
    "express": "^4.16.3",
    "express-request-language": "^1.1.15",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "lingui-react": "^1.4.1",
    "raf": "^3.4.0",
    "react": "^16.4.1",
    "react-apollo": "^2.1.6",
    "react-dom": "^16.4.2",
    "react-emotion": "^9.2.4",
    "react-helmet": "^5.2.0",
    "react-promise": "^2.0.3",
    "react-redux": "^5.0.7",
    "react-universal-component": "^2.9.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-first-router": "^1.9.15",
    "redux-first-router-link": "^1.4.2",
    "redux-form": "^7.4.2",
    "webpack-flush-chunks": "^2.0.0"
  },
  "devDependencies": {
    "autodll-webpack-plugin": "^0.3.9",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-emotion": "^9.2.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-universal-import": "^3.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-lingui-react": "^1.0.13",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-watch": "^2.0.7",
    "cross-env": "^5.2.0",
    "enzyme": "~3.3.0",
    "enzyme-adapter-react-16": "~1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "lingui-cli": "^1.4.4",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.5",
    "react-hot-loader": "^3.1.3",
    "rimraf": "^2.6.2",
    "stats-webpack-plugin": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-hot-server-middleware": "^0.5.0",
    "write-file-webpack-plugin": "^4.3.2"
  },
  "jest": {
    "verbose": true,
    "setupTestFrameworkScriptFile": "<rootDir>/.jest.setup.js"
  },
  "lingui": {
    "localeDir": "<rootDir>/src/components/locale",
    "srcPathDirs": [
      "<rootDir>/src"
    ]
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --single-quote --semi=false --write",
        "eslint --fix",
        "git add"
      ]
    }
  }
}
