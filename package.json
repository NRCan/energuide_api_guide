{
  "name": "proof-of-concept-v2",
  "description": "",
  "version": "1.0.0",
  "main": "server/index.js",
  "author": "Mike Willamson",
  "license": "MIT",
  "scripts": {
    "extract": "lingui extract",
    "compile": "lingui compile",
    "add-locale": "lingui add-locale",
    "dev": "npm run clean && cross-env NODE_ENV=development babel-watch server/index.js",
    "start": "npm run serve",
    "serve": "cross-env NODE_ENV=production node buildServer/index.js",
    "build": "npm run build:client && npm run build:server && npm run build:node",
    "build:client": "rimraf buildClient && cross-env NODE_ENV=production webpack --progress -p --config webpack/client.prod.js",
    "build:server": "rimraf buildServer && cross-env NODE_ENV=production webpack --progress -p --config webpack/server.prod.js",
    "build:node": "cross-env NODE_ENV=production babel server/index.js -o buildServer/index.js",
    "clean": "rimraf buildClient buildServer",
    "precommit": "lint-staged",
    "cm": "git-cz",
    "lint": "eslint src server webpack",
    "format": "prettier --single-quote --semi=false --write '{src,server,webpack}/**/*.js' && npm run lint",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "@cdssnc/gcui": "^0.0.16",
    "apollo-cache-inmemory": "^1.1.0",
    "apollo-client": "^2.0.2",
    "apollo-link-http": "^1.1.0",
    "babel-polyfill": "^6.23.0",
    "emotion": "^8.0.11",
    "emotion-server": "^8.0.11",
    "express": "^4.15.2",
    "facepaint": "^1.1.2",
    "fixed-data-table-2": "^0.8.11",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "history": "^4.6.3",
    "isomorphic-fetch": "^2.2.1",
    "lingui-react": "^1.4.1",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-apollo": "^2.0.0",
    "react-dom": "^16.0.0",
    "react-emotion": "^9.0.2",
    "react-redux": "^5.0.5",
    "react-universal-component": "^2.5.0",
    "redux": "^3.7.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-first-router": "^1.9.15",
    "redux-first-router-link": "^1.1.4",
    "redux-form": "^7.3.0",
    "webpack-flush-chunks": "^1.1.22"
  },
  "devDependencies": {
    "autodll-webpack-plugin": "^0.3.2",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-emotion": "^9.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-universal-import": "^1.2.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-lingui-react": "^1.0.10",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-watch": "^2.0.6",
    "bluebird": "^3.5.1",
    "commitizen": "^2.9.6",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.0.0",
    "enzyme": "~3.3.0",
    "enzyme-adapter-react-16": "~1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-css-chunks-webpack-plugin": "^2.0.15",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lingui-cli": "^1.4.4",
    "lint-staged": "^5.0.0",
    "prettier": "^1.11.1",
    "react-hot-loader": "^3.1.1",
    "rimraf": "^2.6.1",
    "stats-webpack-plugin": "^0.6.1",
    "travis-github-status": "^1.4.0",
    "webpack": "^3.5.4",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-hot-server-middleware": "^0.2.0",
    "write-file-webpack-plugin": "^4.1.0"
  },
  "jest": {
    "verbose": true,
    "setupTestFrameworkScriptFile": "<rootDir>/.jest.setup.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lingui": {
    "localeDir": "<rootDir>/src/components/locale",
    "srcPathDirs": [
      "<rootDir>/src"
    ]
  },
  "lint-staged": {
    "linters": {
      "*.js": [
        "prettier --single-quote --semi=false --write",
        "eslint --fix",
        "git add"
      ]
    }
  }
}
